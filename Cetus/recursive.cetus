Void printf(String format, Int... values);

Foo
{
	Int a;
	Int b;
	
	Foo New(Int a, Int b)
	{
		Foo this;
		this.a = a;
		this.b = b;
		Return this;
	};
	
	&Int A(&Foo this)
	{
		Return this.a;
	};
};

Void main()
{
	Foo foo = Foo.New(2, 3);
	printf("Starting at %i\n", Foo.A(foo));
	
	While (Foo.A(foo) < 6)
	{
		Foo.A(foo) = Foo.A(foo) + 1;
		printf("Loop %i\n", Foo.A(foo));
	};
	
	printf("Ending at %i\n", Foo.A(foo));
	Return;
};