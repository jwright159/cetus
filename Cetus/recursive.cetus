extern Void printf(String format, Int... values);

Void main()
{
    Declare("a", 3);
    printf("Starting at %i\n", a);
    
    While({
        Return(a < 6);
    },
    {
        Assign(a, a + 1);
        printf("Loop %i\n", a);
        ReturnVoid();
    });
    
    printf("Ending at %i\n", a);
    ReturnVoid();
}